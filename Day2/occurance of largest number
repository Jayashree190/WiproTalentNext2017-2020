import java.util.*;
import java.io.*;
public class Main
{
	public static void main(String[] args) 
	{
		int max=0,n=5,count=0;
		Scanner sc = new Scanner(System.in);
		int a[] = new int[5];
		for(int i=0;i<n;i++)
		{
		    a[i]=sc.nextInt();
		    if(a[i]>max)
		      max=a[i];
		}
		for(int i=0;i<n;i++)
		{
		    if(a[i]==max)
		      count++;
		}
		System.out.println(max);
		System.out.print(count);
	}
}
