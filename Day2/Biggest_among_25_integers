import java.util.*;
import java.io.*;
public class Main
{
	public static void main(String[] args) 
	{
		int max=0,n=25;
		Scanner sc = new Scanner(System.in);
		int a[] = new int[25];
		for(int i=0;i<n;i++)
		{
		    a[i]=sc.nextInt();
		    if(a[i]>max)
		      max=a[i];
		}
		System.out.print(max);
	}
}
